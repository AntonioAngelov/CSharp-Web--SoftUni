@model CarDealer.App.Models.Suppliers.SuppliersModel

@{
    ViewData["Title"] = $"{Model.Type} Suppliers";
}

<h1>@ViewData["Title"]</h1>

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Number Of Parts</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var suppler in Model.Supplers)
        {
            <tr>
                <td>@suppler.Id</td>
                <td>@suppler.Name</td>
                <td>@suppler.Parts</td>
            </tr>
        }

    </tbody>
</table>